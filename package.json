{
  "name": "@zyto/workflow-exec",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Execution engine for Zyto (runs steps across Zapier/n8n).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "tsx watch src/worker.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "start": "node dist/worker.js",
    "prepare": "husky install"
  },
  "engines": { "node": ">=20.0.0" },
  "dependencies": {
    "axios": "^1.7.7",
    "ioredis": "^5.4.1",
    "p-retry": "^6.2.0",
    "p-queue": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
